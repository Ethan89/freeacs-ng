#
# freeacs-ng uci configuration
#

config scgi
	option address '127.0.0.1'
	option port '9000'

config amqp
	option host '127.0.0.1'
	option port '5672'
	option user 'guest'
	option pass 'guest'
	option virtual_host '/'

config amqp_exchange
	option broadcast 'freeacs-ng broadcast fanout'
	option provisioning 'freeacs-ng provisioning fanout'

config amqp_queue
	option broadcast 'freeacs-ng broadcast main'
	option provisioning '@remote_addr'
